@model IEnumerable<TeleTimeTest.Models.WorkYear>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.YearID)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.YearID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.YearID }) |
            @Html.ActionLink("Details", "Details", new { id=item.YearID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.YearID })
        </td>
    </tr>
}

</table>
